{
  "name": "@finishit/api",
  "version": "0.1.21",
  "private": true,
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "migrate:up": "tsx src/scripts/migrate.ts up",
    "migrate:down": "tsx src/scripts/migrate.ts down",
    "migrate:create": "node-pg-migrate create --timestamp",
    "backfill:embeddings": "tsx src/scripts/backfillEmbeddings.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.555.0",
    "@aws-sdk/s3-request-presigner": "^3.555.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.18.3",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "pino": "^9.0.0",
    "pino-http": "^9.0.0",
    "redis": "^4.7.0",
    "sharp": "^0.33.3",
    "socket.io": "^4.7.5",
    "zod": "^3.23.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "node-pg-migrate": "^8.0.4",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2",
    "ultracite": "^7.1.4"
  }
}

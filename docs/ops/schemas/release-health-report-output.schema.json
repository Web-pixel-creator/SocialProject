{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://github.com/Web-pixel-creator/SocialProject/blob/main/docs/ops/schemas/release-health-report-output.schema.json",
  "title": "Release Health Report Output",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "schemaPath",
    "schemaVersion",
    "generatedAtUtc",
    "repository",
    "workflow",
    "run",
    "summary",
    "jobs",
    "artifactChecks",
    "artifacts",
    "smokeReport"
  ],
  "properties": {
    "schemaPath": {
      "const": "docs/ops/schemas/release-health-report-output.schema.json"
    },
    "schemaVersion": {
      "const": "1.3.0"
    },
    "generatedAtUtc": {
      "type": "string",
      "format": "date-time"
    },
    "repository": {
      "type": "string",
      "minLength": 1
    },
    "workflow": {
      "$ref": "#/$defs/workflow"
    },
    "run": {
      "$ref": "#/$defs/run"
    },
    "summary": {
      "$ref": "#/$defs/summary"
    },
    "jobs": {
      "$ref": "#/$defs/jobs"
    },
    "artifactChecks": {
      "$ref": "#/$defs/artifactChecks"
    },
    "artifacts": {
      "type": "array",
      "items": {
        "$ref": "#/$defs/artifact"
      }
    },
    "smokeReport": {
      "$ref": "#/$defs/smokeReport"
    },
    "externalChannelFailureModes": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "$ref": "#/$defs/externalChannelFailureModes"
        }
      ]
    }
  },
  "$defs": {
    "nullableDateTime": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time"
    },
    "workflow": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "file",
        "profile"
      ],
      "properties": {
        "file": {
          "type": "string",
          "minLength": 1
        },
        "profile": {
          "type": "string",
          "minLength": 1
        }
      }
    },
    "run": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "id",
        "runNumber",
        "status",
        "conclusion",
        "htmlUrl",
        "headSha",
        "event",
        "createdAt",
        "updatedAt"
      ],
      "properties": {
        "id": {
          "type": "integer",
          "minimum": 1
        },
        "runNumber": {
          "type": [
            "integer",
            "null"
          ],
          "minimum": 1
        },
        "status": {
          "type": [
            "string",
            "null"
          ]
        },
        "conclusion": {
          "type": [
            "string",
            "null"
          ]
        },
        "htmlUrl": {
          "type": [
            "string",
            "null"
          ]
        },
        "headSha": {
          "type": [
            "string",
            "null"
          ]
        },
        "event": {
          "type": [
            "string",
            "null"
          ]
        },
        "createdAt": {
          "$ref": "#/$defs/nullableDateTime"
        },
        "updatedAt": {
          "$ref": "#/$defs/nullableDateTime"
        }
      }
    },
    "summary": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "pass",
        "reasons",
        "requiredJobsTotal",
        "requiredJobsPassed",
        "requiredArtifactsTotal",
        "requiredArtifactsPresent",
        "failedJobsTotal"
      ],
      "properties": {
        "pass": {
          "type": "boolean"
        },
        "reasons": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "requiredJobsTotal": {
          "type": "integer",
          "minimum": 0
        },
        "requiredJobsPassed": {
          "type": "integer",
          "minimum": 0
        },
        "requiredArtifactsTotal": {
          "type": "integer",
          "minimum": 0
        },
        "requiredArtifactsPresent": {
          "type": "integer",
          "minimum": 0
        },
        "failedJobsTotal": {
          "type": "integer",
          "minimum": 0
        }
      }
    },
    "jobs": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "required",
        "failedJobs",
        "totalJobs"
      ],
      "properties": {
        "required": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/requiredJob"
          }
        },
        "failedJobs": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "totalJobs": {
          "type": "integer",
          "minimum": 0
        }
      }
    },
    "requiredJob": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "name",
        "present",
        "pass",
        "status",
        "conclusion",
        "htmlUrl"
      ],
      "properties": {
        "name": {
          "type": "string",
          "minLength": 1
        },
        "present": {
          "type": "boolean"
        },
        "pass": {
          "type": "boolean"
        },
        "status": {
          "type": [
            "string",
            "null"
          ]
        },
        "conclusion": {
          "type": [
            "string",
            "null"
          ]
        },
        "htmlUrl": {
          "type": [
            "string",
            "null"
          ]
        }
      }
    },
    "artifact": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "id",
        "name",
        "expired",
        "sizeInBytes",
        "createdAt",
        "updatedAt",
        "archiveDownloadUrl"
      ],
      "properties": {
        "id": {
          "type": "integer",
          "minimum": 1
        },
        "name": {
          "type": "string",
          "minLength": 1
        },
        "expired": {
          "type": "boolean"
        },
        "sizeInBytes": {
          "type": "integer",
          "minimum": 0
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "updatedAt": {
          "type": "string",
          "format": "date-time"
        },
        "archiveDownloadUrl": {
          "type": "string",
          "format": "uri"
        }
      }
    },
    "requiredArtifactCheck": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "name",
        "present",
        "expired",
        "artifactId"
      ],
      "properties": {
        "name": {
          "type": "string",
          "minLength": 1
        },
        "present": {
          "type": "boolean"
        },
        "expired": {
          "type": "boolean"
        },
        "artifactId": {
          "type": [
            "integer",
            "null"
          ],
          "minimum": 1
        }
      }
    },
    "artifactChecks": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "required",
        "missing"
      ],
      "properties": {
        "required": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/requiredArtifactCheck"
          }
        },
        "missing": {
          "type": "array",
          "items": {
            "type": "string",
            "minLength": 1
          }
        }
      }
    },
    "externalChannelModeDistribution": {
      "type": "object",
      "additionalProperties": {
        "type": "integer",
        "minimum": 0
      }
    },
    "externalChannelAnalysisError": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "runId",
        "message"
      ],
      "properties": {
        "runId": {
          "type": "integer",
          "minimum": 1
        },
        "message": {
          "type": "string",
          "minLength": 1
        }
      }
    },
    "externalChannelAnalyzedRun": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "artifactId",
        "checkedAtUtc",
        "checksTotal",
        "conclusion",
        "failedChannelsTotal",
        "htmlUrl",
        "modeDistribution",
        "pass",
        "requiredChannelsPass",
        "requiredFailedChannelsTotal",
        "runId",
        "runNumber",
        "skipped"
      ],
      "properties": {
        "artifactId": {
          "type": "integer",
          "minimum": 1
        },
        "checkedAtUtc": {
          "$ref": "#/$defs/nullableDateTime"
        },
        "checksTotal": {
          "type": "integer",
          "minimum": 0
        },
        "conclusion": {
          "type": [
            "string",
            "null"
          ]
        },
        "failedChannelsTotal": {
          "type": "integer",
          "minimum": 0
        },
        "htmlUrl": {
          "type": [
            "string",
            "null"
          ]
        },
        "modeDistribution": {
          "$ref": "#/$defs/externalChannelModeDistribution"
        },
        "pass": {
          "type": "boolean"
        },
        "requiredChannelsPass": {
          "type": "boolean"
        },
        "requiredFailedChannelsTotal": {
          "type": "integer",
          "minimum": 0
        },
        "runId": {
          "type": "integer",
          "minimum": 1
        },
        "runNumber": {
          "type": [
            "integer",
            "null"
          ],
          "minimum": 1
        },
        "skipped": {
          "type": "boolean"
        }
      }
    },
    "externalChannelFailureModes": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "artifactName",
        "currentRunId",
        "pass",
        "reasons",
        "windowSize",
        "minimumRuns",
        "analyzedRuns",
        "modeDistribution",
        "channelModeDistribution",
        "nonPassModes",
        "runsWithFailures",
        "runsWithRequiredFailures",
        "missingArtifactRunIds",
        "analysisErrors",
        "generatedAtUtc"
      ],
      "properties": {
        "artifactName": {
          "type": "string",
          "minLength": 1
        },
        "currentRunId": {
          "type": "integer",
          "minimum": 1
        },
        "pass": {
          "type": "boolean"
        },
        "reasons": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "windowSize": {
          "type": "integer",
          "minimum": 1
        },
        "minimumRuns": {
          "type": "integer",
          "minimum": 1
        },
        "analyzedRuns": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/externalChannelAnalyzedRun"
          }
        },
        "modeDistribution": {
          "$ref": "#/$defs/externalChannelModeDistribution"
        },
        "channelModeDistribution": {
          "$ref": "#/$defs/externalChannelModeDistribution"
        },
        "nonPassModes": {
          "type": "array",
          "items": {
            "type": "string",
            "minLength": 1
          }
        },
        "runsWithFailures": {
          "type": "array",
          "items": {
            "type": "integer",
            "minimum": 1
          }
        },
        "runsWithRequiredFailures": {
          "type": "array",
          "items": {
            "type": "integer",
            "minimum": 1
          }
        },
        "missingArtifactRunIds": {
          "type": "array",
          "items": {
            "type": "integer",
            "minimum": 1
          }
        },
        "analysisErrors": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/externalChannelAnalysisError"
          }
        },
        "generatedAtUtc": {
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "smokeSummary": {
      "type": "object",
      "additionalProperties": true,
      "required": [
        "pass",
        "totalSteps",
        "failedSteps"
      ],
      "properties": {
        "pass": {
          "type": "boolean"
        },
        "totalSteps": {
          "type": "integer",
          "minimum": 0
        },
        "failedSteps": {
          "type": "integer",
          "minimum": 0
        }
      }
    },
    "smokeReport": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "source",
        "path",
        "generatedAtUtc",
        "summary"
      ],
      "properties": {
        "source": {
          "type": "string",
          "enum": [
            "local",
            "local-launch-gate",
            "unavailable"
          ]
        },
        "path": {
          "type": "string",
          "minLength": 1
        },
        "generatedAtUtc": {
          "$ref": "#/$defs/nullableDateTime"
        },
        "summary": {
          "oneOf": [
            {
              "type": "null"
            },
            {
              "$ref": "#/$defs/smokeSummary"
            }
          ]
        },
        "fetchError": {
          "type": "string"
        }
      }
    }
  }
}
